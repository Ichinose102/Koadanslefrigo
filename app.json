{
  "expo": {
    "name": "Koadanslefrigo",
    "slug": "koadanslefrigo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tonnom.koadanslefrigo",
      "infoPlist": {
        "NSCameraUsageDescription": "L'application a besoin d'accéder à la caméra pour scanner les codes-barres des aliments."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tonnom.koadanslefrigo",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Autorisez Koadanslefrigo à utiliser votre caméra pour scanner vos produits."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b0160488-13ef-4e19-aebb-8f5552256634"
      }
    }
  }
}
